{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "iam:AttachGroupPolicy",
        "iam:AttachUserPolicy",
        "iam:DetachGroupPolicy",
        "iam:DetachUserPolicy"
      ],
      "Resource": "*",
      "Condition": {
        "ArnEquals": {
          "iam:PolicyArn": "arn:aws:iam::quarantteam:policy/MyProjectS3Access"
        }
      }
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": [
        "iam:ListPolicies",
        "iam:DeleteAccessKey",
        "iam:GetPolicy",
        "iam:AttachUserPolicy",
        "iam:UpdateAccessKey",
        "iam:DeleteUser",
        "iam:ListUserPolicies",
        "iam:CreateUser",
        "iam:CreateAccessKey",
        "iam:CreatePolicy",
        "iam:CreateLoginProfile",
        "iam:*role*",
        "iam:ListGroupsForUser",
        "iam:ListAttachedUserPolicies",
        "iam:ListGroups",
        "iam:CreatePolicyVersion",
        "iam:DeleteLoginProfile",
        "iam:GetAccountSummary"
      ],
      "Resource": "*"
    }
  ]
}
